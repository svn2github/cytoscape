<?xml version="1.0" encoding="UTF-8" ?>
<document componentFinder="A_COUPLE_OF_CLASS_AND_LABEL_IS_EQUAL"
	windowFinder="CLASS_IS_EQUAL_STRICTLY"
	windowClass="cytoscape.view.CytoscapeDesktop">

	<!--  
		Scinario 1: Load Excel network file.
	-->
	<scenario name="IMPORT_NETWORK_EXCEL_FILE">
		<Click componentClass="JMenu" label="File" />
		<Click componentClass="JMenuItem" label="Import" />
		<Click componentClass="JMenuItem"
			label="Network from Table (Text/MS Excel)..." />
		<retriever retryTime="2000">
			<Click label="Select File" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>
		<retriever retryTime="2000">
			<FileChooser dialogTitle="Select local file"
				directory="testData" fileName="yeastHQ_sub.xls" waitTime="100" />
		</retriever>

		<Click componentClass="JCheckBox"
			label="Show Text File Import Options"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<Spinner upside="true" clickCount="3" name="startRowSpinner"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<Click label="Reload" componentClass="JButton"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<Click componentClass="JCheckBox"
			label="Transfer first line as attribute names"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="1" name="sourceComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="2" name="interactionComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="3" name="targetComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="3"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="4"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<retriever retryTime="2000">
			<Click label="Import" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>

		<Delay ms="1500" />
		<retriever retryTime="10000" intervalTime="1000">
			<if verifier="Verify" property="enabled" expected="true"
				componentClass="JButton" label="    Close    "
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="cytoscape.task.ui.JTask">
				<Click label="    Close    " componentClass="JButton"
					windowFinder="CLASS_IS_ACCESSIBLE"
					windowClass="cytoscape.task.ui.JTask" />
			</if>
		</retriever>
		<Delay ms="500" />
	</scenario>


	<!--  
		Scinario 2: Load text network file.
	-->
	<scenario name="IMPORT_NETWORK_TEXT_TABLE">
		<Click componentClass="JMenu" label="File" />
		<Click componentClass="JMenuItem" label="Import" />
		<Click componentClass="JMenuItem"
			label="Network from Table (Text/MS Excel)..." />
		<retriever retryTime="2000">
			<Click label="Select File" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>
		<retriever retryTime="2000">
			<FileChooser dialogTitle="Select local file"
				directory="testData" fileName="galFiltered_EAttr.txt"
				waitTime="100" />
		</retriever>

		<Click componentClass="JCheckBox"
			label="Show Text File Import Options"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<Click componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			name="commentLineTextField" componentClass="JTextField"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<TypeText text="!" />

		<Click label="Reload" componentClass="JButton"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<Click componentClass="JCheckBox" label="Space"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<Click componentClass="JCheckBox"
			label="Transfer first line as attribute names"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="1" name="sourceComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="3" name="interactionComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<ComboBox targetIndex="2" name="targetComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="3"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="4"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="5"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<TableHeader componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JTable" column="6"
			windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />


		<Click mouseButton="BUTTON3" clickCount="1" x="650" y="5"
			componentFinder="CLASS_IS_EQUAL_STRICTLY"
			componentClass="JScrollPane" windowFinder="CLASS_IS_EQUAL_STRICTLY"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<Delay ms="200" />

		<Click componentClass="JRadioButton" label="List:"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.AttributeTypeDialog" />

		<ComboBox targetIndex="4" name="listDelimiterComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.AttributeTypeDialog" />

		<Click componentClass="JButton" label="OK"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.AttributeTypeDialog" />

		<retriever retryTime="2000">
			<Click label="Import" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>

		<Delay ms="1500" />
		<retriever retryTime="10000" intervalTime="1000">
			<if verifier="Verify" property="enabled" expected="true"
				componentClass="JButton" label="    Close    "
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="cytoscape.task.ui.JTask">
				<Click label="    Close    " componentClass="JButton"
					windowFinder="CLASS_IS_ACCESSIBLE"
					windowClass="cytoscape.task.ui.JTask" />
			</if>
		</retriever>
		<Delay ms="500" />
	</scenario>

	<!--  
		Scinario 3: Load Node Attributes from text file.
	-->
	<scenario name="IMPORT_EDGE_ATTRIBUTE_TEXT_FILE">
		<Click componentClass="JMenu" label="File" />
		<Click componentClass="JMenuItem" label="Import" />
		<Click componentClass="JMenuItem"
			label="Attribute from Table (Text/MS Excel)..." />

		<Click componentClass="JRadioButton" label="Edge"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<retriever retryTime="2000">
			<Click label="Select File" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>
		<retriever retryTime="2000">
			<FileChooser dialogTitle="Select local file"
				directory="testData" fileName="galFilteredEdgeAttr.txt"
				waitTime="100" />
		</retriever>

		<Click componentClass="JCheckBox"
			label="Show Text File Import Options"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<Click componentClass="JCheckBox"
			label="Transfer first line as attribute names"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />

		<retriever retryTime="2000">
			<Click label="Import" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>

		<Delay ms="1000" />
		<retriever retryTime="10000" intervalTime="1000">
			<if verifier="Verify" property="enabled" expected="true"
				componentClass="JButton" label="    Close    "
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="cytoscape.task.ui.JTask">
				<Click label="    Close    " componentClass="JButton"
					windowFinder="CLASS_IS_ACCESSIBLE"
					windowClass="cytoscape.task.ui.JTask" />
			</if>
		</retriever>
		<Delay ms="500" />
	</scenario>

	<!--  
		Scinario 4: Load GO and GA for yeast network.
	-->
	<scenario name="IMPORT_GO_AND_GA">
		<Click componentClass="JMenu" label="File" />
		<Click componentClass="JMenuItem" label="Import" />
		<Click componentClass="JMenuItem"
			label="Ontology and Annotation..." />

		<ComboBox targetIndex="1" name="annotationComboBox"
			componentFinder="A_COUPLE_OF_CLASS_AND_NAME_IS_EQUAL"
			windowFinder="CLASS_IS_ACCESSIBLE"
			windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		<Delay ms="2000" />
		<retriever retryTime="2000">
			<Click label="Import" componentClass="JButton"
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="edu.ucsd.bioeng.coreplugin.tableImport.ui.ImportTextTableDialog" />
		</retriever>
		<Delay ms="10000" />
		<retriever retryTime="10000" intervalTime="1000">
			<if verifier="Verify" property="enabled" expected="true"
				componentClass="JButton" label="    Close    "
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="cytoscape.task.ui.JTask">
				<Click label="    Close    " componentClass="JButton"
					windowFinder="CLASS_IS_ACCESSIBLE"
					windowClass="cytoscape.task.ui.JTask" />
			</if>
		</retriever>
		<Delay ms="1000" />
	</scenario>

	<!--  
		Scinario 6: Write the results as session file.
	-->
	<scenario name="WRITE_SESSION">
		<Click componentClass="JMenu" label="File" />
		<Click componentClass="JMenuItem" label="Save As..." />

		<Delay ms="1000" />


		<TypeText text="testSession.cys" />
		<KeyPress keyCodeString="TAB" />
		<KeyRelease keyCodeString="TAB" />
		<Delay ms="100" />
		<KeyPress keyCodeString="TAB" />
		<KeyRelease keyCodeString="TAB" />
		<Delay ms="100" />
		<KeyPress keyCodeString="SPACE" />
		<KeyRelease keyCodeString="SPACE" />

		<Delay ms="10000" />
		<retriever retryTime="10000" intervalTime="1000">
			<if verifier="Verify" property="enabled" expected="true"
				componentClass="JButton" label="    Close    "
				windowFinder="CLASS_IS_ACCESSIBLE"
				windowClass="cytoscape.task.ui.JTask">
				<Click label="    Close    " componentClass="JButton"
					windowFinder="CLASS_IS_ACCESSIBLE"
					windowClass="cytoscape.task.ui.JTask" />
			</if>
		</retriever>
		<Delay ms="10000" />
	</scenario>

</document>
