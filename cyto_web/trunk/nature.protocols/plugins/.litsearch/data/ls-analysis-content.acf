      <title value="Connections between single-cell biomechanics and human disease states: gastrointestinal cancer and malaria." />
      <authors value="Suresh S,Spatz J,Mills JP,Micoulet A,Dao M,Lim CT,Beil M,Seufferlein T" />
      <PubEntryImpl>
         <name value="Acta Biomater" />
